[core]
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
  editor = nvim
[github]
  user = Jeremiah Tabb
  email = jollyjerr@gmail.com
[user]
  name = Jeremiah Tabb
  email = jollyjerr@gmail.com
[color]
  ui = true
  diff = auto
  status = auto
  branch = auto
[format]
  pretty = %C(yellow)%h%Creset %s %C(red)(%an, %cr)%Creset
[push]
  default = current
[difftool "sourcetree"]
  cmd = opendiff \"$LOCAL\" \"$REMOTE\"
  path =
[credential]
  helper = osxkeychain
[pull]
  rebase = false
[init]
  defaultBranch = main
