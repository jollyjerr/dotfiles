[core]
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
  editor = nvim
[github]
  user = Jeremiah Tabb
  email = jollyjerr@gmail.com
[user]
  name = Jeremiah Tabb
  email = jollyjerr@gmail.com
[color]
  ui = true
  diff = auto
  status = auto
  branch = auto
[format]
  pretty = %C(yellow)%h%Creset %s %C(red)(%an, %cr)%Creset
[push]
  default = current
[pull]
  rebase = false
[credential]
  helper = osxkeychain
[init]
  defaultBranch = main
[difftool]
    prompt = true
[diff]
    tool = nvimdiff
[difftool "nvimdiff"]
    cmd = "nvim -d \"$LOCAL\" \"$REMOTE\""
